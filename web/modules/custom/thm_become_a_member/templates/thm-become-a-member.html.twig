{#
/**
 * @file
 * Template for the Membership (\become-a-member) page.
 *
 * Available variables:
 * - signup_form: Button that kicks off the "BasicMaker" flow
 * - payment_form: Button that kicks off the "DigitalMaker" flow
 * - industry_form: Button that kicks off the "IndustryMaker" flow
 *
 * @ingroup themable
 *
 * Available buttons
 * - {{ signup_form }}
 * - {{ payment_form }}
 * - {{ industry_form }}
 */
#}

<hr>
<div class="membership row">
  <h2 class="two-column-testimonial--header col-12">Pricing</h2>
  <section class="col-6 membership--individual row">
    {# SVG #}
    <div class="col-12">
      <h3>Individual</h3>
      <svg id="689ba78a-a3d4-4e7f-b232-31a6e0fbeeff" data-name="Layer 1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 422 422"><title>individual-icon</title><image id="8645c9aa-bf8d-4e2a-befa-ba00f75be244" data-name="individual icon" width="422" height="422" xlink:href="data:image/png;base64," style="isolation:isolate"/></svg>
    </div>
    <div class="col-6 membership--outline-box">
      <h4>BasicMaker</h4>
      <p>$0</p>
      {{ signup_form }}
    </div>
    <div class="col-6 membership--outline-box">
      <h4>DigitalMaker</h4>
      <p>$30/month</p>
      {{ payment_form }}
    </div>
  </section>
  <section class="col-6 membership--institutional row">
    {# SVG #}
    <div class="col-12">
      <h3>Institutional</h3>
      <svg id="4c1dcfb6-5132-41b3-a094-5245c87c45d0" data-name="Layer 1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 422 422"><title>group-icon</title><image id="15ad35f0-c2ee-4dc8-807a-3cbe52fb617a" data-name="group icon" width="422" height="422" xlink:href="data:image/png;base64," style="isolation:isolate"/></svg>
    </div>
    <div class="col-12 membership--outline-box">
      <h4>InstitutionalMaker</h4>
      {{ industry_form }}
    </div>
  </section>
</div>